{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Cadre Creator",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Cadre Creator",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "TauSeptIcon.ico",
      "TauSeptIcon.png"
    ]
  }
}
